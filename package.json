{
  "projects": {
    "apps/frontend": {
      "buildCommand": "yarn build",
      "outputDirectory": "dist"
    }
  },
  "builds": [
    {
      "src": "apps/frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "apps/frontend/dist/$1"
    }
  ]
}
